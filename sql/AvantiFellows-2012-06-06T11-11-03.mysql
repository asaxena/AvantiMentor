INSERT INTO `content_type_profile` VALUES ('204','204','Vishal','Fellow',null,null,null,'203','1','a:2:{s:3:\"alt\";s:0:\"\";s:5:\"title\";s:0:\"\";}','Tyagi',null,null,'+917417959674','2011-08-02T00:00:00',null,null,null,null,null,null,null,'He belongs ao an middle-class.His father works in Govt. hospital and mother is a housewife. He has one sister besides his parents in the rented house ,having two rooms only.Monetary problem exist to some extent.Mother is very caring and determined to shape her children\'s future.Academically her sister is also intelligent, so they manage to study in company.','He is very consistent and open minded. He is very dedicated ,as all the tasks I have given to him he has completed in time.He is diligent and dedicated to his work.He is very soft in behavior.He study mostly in morning times,which is the best time for studies.He plays cricket and also has good skills in joking.','He wants to become an electrical engineer in future.Beyond this, he has no knowledge of different fields.',null,null,null),('205','205','Fellow3','Fellow',null,null,null,null,null,null,'IITK',null,null,'+91','2011-08-02T00:00:00',null,null,null,null,null,null,null,null,null,null,null,null,null),('206','206','Bhavesh ','Mentor',null,null,null,'135','1','a:2:{s:3:\"alt\";s:0:\"\";s:5:\"title\";s:0:\"\";}','Gore',null,null,'9028209661','1991-04-07T00:00:00',null,'Hostel 3,\r\nIIT Bombay.',null,null,null,null,null,null,null,null,null,null,null),('207','207','Kartik','Mentor',null,null,null,null,null,null,'Kothari',null,null,'09869082361','1992-08-13T00:00:00',null,null,null,null,null,null,null,null,null,null,null,null,null),('209','209','Shivani','Fellow',null,null,null,null,null,null,'Choudhary',null,null,'+91','2011-08-04T00:00:00',null,null,null,null,null,null,null,null,null,null,null,null,null),('210','210','Ashish','Fellow',null,null,null,null,null,null,'Sharma',null,null,'+91','2011-08-04T00:00:00',null,null,null,null,null,null,null,null,null,null,null,null,nul